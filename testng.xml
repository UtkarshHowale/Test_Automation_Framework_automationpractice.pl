<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Automation Framework Eexcution Suite ">
	<test name="UI Automation Tests" parallel="methods" thread-count="8"
		verbose="1">
		<parameter name="browser" value="${browser}"></parameter>
		<parameter name="isLambdaTest" value="${isLambdaTest}"></parameter>
		<parameter name="isHeadless" value="${isHeadless}"></parameter>
		<classes>
			<class name="com.ui.tests.ValidCreadLoginTest" />
			<class name="com.ui.tests.InvalidCreadLoginTest" />
			<class name="com.ui.tests.SearchProductTest" />
			<class name="com.ui.tests.AddFirstAddressTest" />
			<class name="com.ui.tests.ProductCheckoutTest" />
		</classes>
	</test> <!--
	Test -->
</suite> <!--
Suite -->
